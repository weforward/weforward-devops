.woolselectfadein-enter-active,.woolselectfadein-leave-active{-webkit-transition:all .2s;transition:all .2s;opacity:1;-webkit-transform:translateY(0);transform:translateY(0)}.woolselectfadein-enter,.woolselectfadein-leave-to{opacity:0;-webkit-transform:translateY(-10px);transform:translateY(-10px)}.wool-select{position:relative;font-size:14px;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;cursor:pointer;z-index:1}.wool-select-placeholder{border:1px solid #afabab;border-radius:3px}.wool-select-placeholder.filter{text-indent:10px}.wool-select-iteminner,.wool-select-itemwrap{width:100%;text-indent:0}.wool-select-itemwrap{top:calc(100% + 1px);width:100%;left:0;background-color:#fff;-webkit-box-sizing:border-box;box-sizing:border-box;-webkit-box-shadow:2px 2px 2px #d4d4d4,-1px 0 1px #f1f1f1;box-shadow:2px 2px 2px #d4d4d4,-1px 0 1px #f1f1f1;position:absolute}.wool-select-customfilter{cursor:text;height:inherit}.wool-select-options{max-height:300px;text-indent:10px}.wool-select-options.wt-scroll::-webkit-scrollbar{width:7px;height:0}.wool-select-optionitem{line-height:40px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}.wool-select-nodata{color:#aaa;padding:10px;line-height:1.2}input.wool-select-search{border:none;width:100%;height:40px;font-size:14px;color:#000;text-indent:10px;outline:none;border-bottom:1px solid #f3f3f3}.wool-select-optionitem:hover{background-color:#f1f1f1}