public class OrganizationnUtil {
	/** 会话对象 */
	private static ThreadLocal<String> SESSION = new ThreadLocal<String>();;

	public static String getOrganization() {
		return SESSION.get();
	}

	public static void setOrganization(String org) {
		SESSION.set(org);
	}
}
